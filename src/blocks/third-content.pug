mixin block-sixth(nametitle)
    a(href="#").block-sixth
        figure.icon-education(class=classimg)
        .block-sixth__title(class=classtitle) #{nametitle}
mixin block-seven(test)
    p(class=`link_${test}`) #{test}
    div(class='name_' + test)

