mixin block-first(classimg,classtitle,nametitle)
    a(href="#").block-first
        .block-first__img(class=classimg)
        .block-first__title(class=classtitle) #{nametitle}
mixin block-second(classimg,classtitle,nametitle)
    a(href="#").block-second
        .block-second__img(class=classimg)
        .block-second__title(class=classtitle) #{nametitle}
mixin block-third(nametitle)
    a(href="#").block-third
        figure.icon-paid-education
        .block-third__title #{nametitle}
mixin block-fourth(classimg,nametitle)
    a(href="#").block-fourth(class=classimg)
        figure.icon-places
        p.block-fourth__title #{nametitle}
mixin block-fifth(classimg,classtitle,nametitle)
